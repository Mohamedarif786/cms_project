<!DOCTYPE html>
<html lang="en">

<head>
	<title>
		<%= pageTitle %>
	</title>
	<%- include("../partials/head") %>
		<script src="https://debk60diwzf0m.cloudfront.net/js/settings.js"></script>
</head>
<!--
  HOW TO USE: 
  data-theme: default (default), dark, light
  data-layout: fluid (default), boxed
  data-sidebar-position: left (default), right
  data-sidebar-behavior: sticky (default), fixed, compact
-->

<body data-theme="default" data-layout="fluid" data-sidebar-position="left" data-sidebar-behavior="sticky">
	<div class="wrapper">
		<%- include("../partials/sidebar") %>
			<div class="main">
				<%- include("../partials/header") %>

					<main class="content p-3">
						<div class="container-fluid ">
							<div class="row" id="msgContent" style="display: none;">
								<div class="alert alert-warning alert-dismissible" role="alert">
									<button type="button" class="btn-close" data-bs-dismiss="alert"
										aria-label="Close"></button>
									<div class="alert-message">
										<p class="mb-0"></p>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-12">
									<div class="card">
										<!--
										<div class="card-header">
											<h5 class="card-title">Manage Resources</h5>
											<h6 class="card-subtitle text-muted">Content</h6>
										</div>
										-->
										<div class="card-header">
											<div class="row">
												<div class="col-md-6">
													<h5 class="card-title">
														<%= title %>
													</h5>
													<h6 class="card-subtitle text-muted"></h6>
												</div>
												<div class="col-md-6">
													<div class="card-actions float-end">
														<button id="add-resource" class="btn btn-outline-primary "><i
																class="align-middle me-2" data-feather="plus"></i>
															Add Club</button>
													</div>
												</div>
											</div>

										</div>

										<div class="card-body">

											<div class="table-responsive">

											 
												<table id="main-table" class="table  table-striped  table-bordered text-nowrap" data-resizable-columns-id="demo-table">
													<thead>
														<tr>
															<!--<th data-search="false" >All</th>data-all="true"-->
															<th>Action</th>
															<th data-search="true">Name</th>
															<th data-search="true">Email</th>
															<th>Address Line 1</th>
															<th>Address Line 2</th>
															<th>City</th>
															<th>State</th>
															<th>Country</th>
															<th>Postal code</th>
															<!--<th>Calling code</th>-->
															<th>Phone</th>
															<th>Short form</th>
															<!--<th>Member collections</th>
															<th>SSO</th>-->

															<!--data-action="true"-->
														</tr>
													</thead>
													<tbody>
													</tbody>
												</table>
											</div>

											<!-- pagination support element (fPageStats display stats if incuded)-->
											<div class="row">
												<div class="col-md-6">
													<span id="fPageStats"></span>
												</div>
												<div class="col-md-6">
													<ul class="pagination float-end" id="fPage"></ul>
												</div>
											</div>

										</div>
									</div>
								</div>
							</div>
						</div>

						<div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-hidden="true">
							<div class="modal-dialog modal-dialog-centered " role="document">
								<div class="modal-content">
									<div class="modal-header">
										<h5 class="modal-title">Add Club</h5>
										<button type="button" class="btn-close" data-bs-dismiss="modal"
											aria-label="Close"></button>
									</div>
									<div class="modal-body">
										<form id="addForm" class="form needs-validation" autocomplete="off">
											<div class="modal-body m-3">
												<div class="mb-3">
													<label class="form-label">Name</label>
													<input class="form-control" id="resourceName" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter name" />
												</div>

												<div class="mb-3">
													<label class="form-label">Email</label>
													<input class="form-control" id="email" type="email"
														autocomplete="off" required custom_validation="true"
														custom_validation_msg="Please enter valid email" />
												</div>
												<div class="mb-3">
													<label class="form-label">Address Line1</label>
													<input class="form-control" id="address1" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter address" />
												</div>
												<div class="mb-3">
													<label class="form-label">Address Line2</label>
													<input class="form-control" id="address2" autocomplete="off" />
												</div>
												<div class="mb-3">
													<label class="form-label">City</label>
													<input class="form-control" id="city" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter city" />
												</div>
												<div class="mb-3">
													<label class="form-label">State</label>
													<input class="form-control" id="state" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter state" />
												</div>
												<div class="mb-3">
													<label class="form-label">Country</label>
													<input class="form-control" id="country" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter country" />
												</div>
												<div class="mb-3">
													<label class="form-label">Postal code</label>
													<input class="form-control" id="postalcode" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter postal code" />
												</div>
												<div class="mb-3">
													<label class="form-label">Calling code</label>
													<input class="form-control" id="callingcode" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter calling code" />
												</div>
												<div class="mb-3">
													<label class="form-label">Phone</label>
													<input class="form-control" id="phone" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter phone" />
												</div>
												<div class="mb-3">
													<label class="form-label">Short form</label>
													<input class="form-control" id="shortform" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter short form" />
												</div>

												<div class="mb-3">
													<label class="form-label">Member collections</label>
													<select id="membercollections" required custom_validation="true"
														custom_validation_msg="Please member collections"
														class="form-control">
														<option value="">--select--</option>
														<option value="IMPORT">IMPORT</option>
														<option value="API">API</option>
														<option value="CMS">CMS</option>
													</select>
												</div>

												<div class="mb-3">
													<label class="form-label">SSO</label>
													<input class="form-control" id="sso" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter SSO" />
												</div>
												<div class="mb-3">
													<label class="form-label">Status</label>
													<div class="form-check form-switch">
														<input class="form-check-input" type="checkbox" checked
															id="status">
													</div>
												</div>

											</div>
										</form>

									</div>
									<div class="modal-footer">
										<button type="button" class="btn btn-secondary"
											data-bs-dismiss="modal">Close</button>
										<button type="button" class="btn btn-primary" id="add-confirm">Save</button>
									</div>
								</div>
							</div>
						</div>

						<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-hidden="true">
							<div class="modal-dialog modal-dialog-centered modal-sm" role="document">
								<div class="modal-content">
									<div class="modal-header">
										<h5 class="modal-title">Delete Club</h5>
										<button type="button" class="btn-close" data-bs-dismiss="modal"
											aria-label="Close"></button>
									</div>
									<div class="modal-body m-3">
										<p class="mb-0">This will delete the content ?</p>
									</div>
									<div class="modal-footer">
										<button type="button" class="btn btn-secondary"
											data-bs-dismiss="modal">Close</button>
										<button type="button" class="btn btn-danger" id="delete-confirm">Delete</button>
									</div>
								</div>
							</div>
						</div>

						<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-hidden="true">
							<div class="modal-dialog modal-dialog-centered " role="document">
								<div class="modal-content">
									<div class="modal-header">
										<h5 class="modal-title">Edit Club</h5>
										<button type="button" class="btn-close" data-bs-dismiss="modal"
											aria-label="Close"></button>
									</div>
									<div class="modal-body">

										<form id="editForm" class="form needs-validation" autocomplete="off">
											<div class="modal-body m-3">
												<div class="mb-3">
													<label class="form-label">Name</label>
													<input class="form-control" id="resourceNameEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter name" />
												</div>

												<div class="mb-3">
													<label class="form-label">Email</label>
													<input class="form-control" id="emailEdit" readonly type="email"
														autocomplete="off" required custom_validation="true"
														custom_validation_msg="Please enter valid email" />
												</div>
												<div class="mb-3">
													<label class="form-label">Address Line1</label>
													<input class="form-control" id="address1Edit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter address" />
												</div>
												<div class="mb-3">
													<label class="form-label">Address Line2</label>
													<input class="form-control" id="address2Edit" autocomplete="off" />
												</div>
												<div class="mb-3">
													<label class="form-label">City</label>
													<input class="form-control" id="cityEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter city" />
												</div>
												<div class="mb-3">
													<label class="form-label">State</label>
													<input class="form-control" id="stateEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter state" />
												</div>
												<div class="mb-3">
													<label class="form-label">Country</label>
													<input class="form-control" id="countryEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter country" />
												</div>
												<div class="mb-3">
													<label class="form-label">Postal code</label>
													<input class="form-control" id="postalcodeEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter postal code" />
												</div>
												<div class="mb-3">
													<label class="form-label">Calling code</label>
													<input class="form-control" id="callingcodeEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter calling code" />
												</div>
												<div class="mb-3">
													<label class="form-label">Phone</label>
													<input class="form-control" id="phoneEdit" autocomplete="off"
														required custom_validation="true"
														custom_validation_msg="Please enter phone" />
												</div>
												<div class="mb-3">
													<label class="form-label">Short form</label>
													<input class="form-control" id="shortformEdit" readonly
														autocomplete="off" required custom_validation="true"
														custom_validation_msg="Please enter short form" />
												</div>

												<div class="mb-3">
													<label class="form-label">Member collections</label>
													<select id="membercollectionsEdit" required custom_validation="true"
														custom_validation_msg="Please member collections"
														class="form-control">
														<option value="">--select--</option>
														<option value="IMPORT">IMPORT</option>
														<option value="API">API</option>
														<option value="CMS">CMS</option>
													</select>
												</div>

												<div class="mb-3">
													<label class="form-label">SSO</label>
													<input class="form-control" id="ssoEdit" autocomplete="off" required
														custom_validation="true"
														custom_validation_msg="Please enter SSO" />
												</div>
												<div class="mb-3">
													<label class="form-label">Status</label>
													<div class="form-check form-switch">
														<input class="form-check-input" type="checkbox" checked
															id="statusEdit">
													</div>
												</div>


												<div class="mb-3">
													<label class="form-label">Update API Keys</label>
													<div class="form-check form-switch">
														<input class="form-check-input" type="checkbox"
															id="xApiKeyEdit">
													</div>
												</div>


											</div>
										</form>

									</div>
									<div class="modal-footer">
										<button type="button" class="btn btn-secondary"
											data-bs-dismiss="modal">Close</button>
										<button type="button" class="btn btn-primary" id="edit-confirm">Update</button>
									</div>
								</div>
							</div>
						</div>
					</main>
					<%- include("../partials/foot") %>
						<%- include("../partials/footer") %>

			</div>
	</div>
	<%- include("../partials/scripts") %>
		<script>
			const _apiBase = '/club';
		</script>
		<script src="js/pagination.js"></script>
		<script src="js/club.js"></script>
		<%- include("../partials/scripts") %>
</body>

</html>