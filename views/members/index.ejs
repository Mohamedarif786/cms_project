            <script>
                let localStorageData = JSON.parse(localStorage.getItem("user"));
                let loggedClubid = localStorageData.clubid;
                if (loggedClubid) {
                    $("#downloadTemplate").attr("href", "/member/download/" + loggedClubid + "/");
                    $("#downloadTemplate").addClass("btn btn btn-outline-primary");
                    $("#downloadTemplate").attr("title", "Download Template");
                    $("#downloadTemplate").attr("target", "_blank");
                    //$("#importMemberDivClubid").hide();
                    $("#importMemberDivClubid").remove();
                    $("#loggedClubidValue").val(loggedClubid);
                }
                else {
                    $("#downloadTemplate").addClass("btn btn btn-outline-primary");
                    $("#downloadTemplate").attr("title", "Download Template");
                    $("#downloadTemplate").attr("data-bs-toggle", "modal");
                    $("#downloadTemplate").attr("data-bs-target", "#DownloadMemberTemplate");
                    //$("#importMemberDivClubid").show();
                    $("#loggedClubidValue").val('');
                }
            </script>
